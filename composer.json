{
  "name": "danielpieper/mrcli",
  "description": "A simple cli app to announce merge requests to slack",
  "keywords": [
    "merge request",
    "slack",
    "gitlab"
  ],
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Pieper",
      "email": "github@daniel-pieper.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DanielPieper\\MergeReminder\\": "src"
    }
  },
  "require-dev": {
    "infection/infection": "^0.7.0",
    "phpunit/phpunit": "^7.0",
    "fzaninotto/faker": "^1.7",
    "squizlabs/php_codesniffer": "^3.2.2",
    "phpmd/phpmd": "^2.6.0",
    "sebastian/phpcpd": "^4.0.0"
  },
  "require": {
    "php": "^7.1",
    "ext-iconv": "*",
    "ext-curl": "*",
    "symfony/console": "^4.0.4",
    "php-http/curl-client": "^1.7",
    "m4tthumphrey/php-gitlab-api": "^9.11",
    "nesbot/carbon": "^2.7",
    "league/container": "^3.2",
    "vlucas/phpdotenv": "^2.5",
    "razorpay/slack": "^1.10"
  },
  "minimum-stability": "stable",
  "bin": [
    "bin/mrcli"
  ]
}
